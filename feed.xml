          &lt;div class=&quot;each-single-image&quot;&gt;
            &lt;img src=&quot;/images/wordpress/3/accelaar-dashboard-0.jpg&quot; alt=&quot;Company Dashboard&quot;&gt;
            &lt;div class=&quot;contents&quot;&gt;
            &lt;h5 class=&quot;title&quot;&gt;Company Dashboard&lt;/h5&gt;
            &lt;/div&gt;
          &lt;/div&gt;
          
          &lt;div class=&quot;each-single-image&quot;&gt;
            &lt;img src=&quot;/images/wordpress/3/accelaar-dashboard-1.jpg&quot; alt=&quot;User Dashboard&quot;&gt;
            &lt;div class=&quot;contents&quot;&gt;
